{
    "PersonaChat": {
        "origin": "crowdsourcing", 
        "source": "huggingface",
        "repo/url": "AlekseyKorshuk/persona-chat",
        "splits": ["train", "validation"],
        "features": ["personality", "utterances"]
    },
    "SyntheticPersonaChat": {
        "origin": "synthetic",
        "source": "huggingface",
        "repo/url": "google/Synthetic-Persona-Chat",
        "splits": ["train", "validation", "test"],
        "features": ["user 1 personas", "user 2 personas", "Best Generated Conversation"]
    },
    "MSC": {
        "origin": "crowdsourcing",
        "source": "huggingface",
        "repo/url": "nayohan/multi_session_chat",
        "splits": ["train", "validation", "test"],
        "features": ["dataset", "dialoug_id", "session_id", "persona1", "persona2", "dialogue", "speaker"]
    },
    "PEC": {
        "origin": "reddit",
        "source": "huggingface",
        "repo/url": "peixiang/pec",
        "splits": ["train", "validation", "test"],
        "features": ["personas", "context", "context_speakers", "response", "response_speaker"],
        "dataset_config": {"name": "all"}
    },
    "FoCus": {
        "origin": "crowdsourcing",
        "source": "gdrive",
        "repo/url": "https://drive.usercontent.google.com/download?id=1YmEW12HqjAjlEfZ05g8VLRux8kyUjdcI&export=download&authuser=0",
        "splits": ["train", "validation"],
        "features": ["dialogID", "persona", "landmark_link", "knowledge", "utterance"]
    },
    "MPChat": {
        "origin": "reddit",
        "source": "gdrive",
        "repo/url": "https://drive.usercontent.google.com/download?id=18bur87ayw_8NkQsCz_UCml0mHfuytlVu&export=download&authuser=0",
        "splits": ["train", "validation", "test"],
        "features": ["subreddit", "messages", "message_ids", "main_author", "authors", "created_utcs", "has_image", "all_personas", "direct_url", "file_name", "gpp_grounded_persona", "gpp_candidate_personas"]
    },
    "PER-CHAT": {
        "origin": "reddit",
        "source": "gdrive",
        "repo/url": "https://drive.usercontent.google.com/download?id=1MkC5FmbRvk81EfpcHF9zASwYsUUKlQ7z&export=download&authuser=0",
        "splits": ["train", "validation", "test"],
        "features": ["src_text", "responses"]
    }
}